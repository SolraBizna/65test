.INCLUDE "header.inc"
.MACRO TestLittle
	LDA #<BigAddr
	STA (\1+$80)&255
	LDA #>BigAddr
	STA (\1+$81)&255
	LDX #$80
.DB $01
.DB \1
	PHA
	PHX
	PHY
	PHP
.ENDM
.MACRO Test
.DEFINE BigAddr \1
	TestLittle $01
	TestLittle $ff
.UNDEF BigAddr
.ENDM
.INCLUDE "util/am_tmpls/big_addresses.inc"
	BRK

.ORGA $00
.DEFINE X 0
.REPEAT 256
.DB X
.REDEFINE X X+1
.ENDR
